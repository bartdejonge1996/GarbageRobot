  <!-- <include file="$(find rtabmap_ros)/launch/rgbd_mapping.launch" >
    <arg name="rtabmap_args"            value="delete_db_on_start"/>
    <arg name="localization"            value="false"/>
    <arg name="rviz"                    value="false" />
